<h1>Recherche d'un Administrateur </h1>
<form action="" method="POST">
    Recherche: <input type="text" name="input_recherche"/>
    <input type="submit" name="rechercher" value="Rechercher"/>
</form>
{% if administrateurs is empty %}
    Aucun administrateur trouvÃ©
{% else %}
    <table border="1" >
        <tr>
            <th>Nom</th>
            <th>Prenom</th>
        </tr>
        {% for administrateur in administrateurs %}
            <tr>
                <td>{{ administrateur.nom }}</td>
                <td>{{ administrateur.prenom }}</td>
            </tr>
        {% endfor %}
    </table>
{% endif %}
